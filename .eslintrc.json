{
    "extends": "airbnb-base",
    "rules": {
        "func-names": "off",
        "no-alert": "off",
        "no-console": "warn",
        "no-plusplus": "off",
        "no-unused-vars": "warn",
        "vars-on-top": "off",
        "no-multi-spaces": "off",
        "key-spacing": "off",
        "max-len": "off",
        "no-undef": "off",
        "no-param-reassign": "off"
    }
}